<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="footer_links">
            <block class="Magento\Framework\View\Element\Html\Link" name="rma-link">
                <arguments>
                    <argument name="label" xsi:type="string">RMA Returns</argument>
                    <argument name="path" xsi:type="string">mprmasystem/guest/allrma</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceContainer name="layout2_seller_account_navigation">
            <block class="Webkul\Marketplace\Block\Account\Navigation" name="mprma_navigation" before="-" template="Webkul_MpRmaSystem::account/layout2/navigation.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
