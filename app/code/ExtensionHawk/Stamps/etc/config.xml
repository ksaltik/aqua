<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <stamps>
                <active>1</active>
                <sallowspecific>0</sallowspecific>
                <model>ExtensionHawk\Stamps\Model\Carrier\Stamps</model>
				<shipment_requesttype>0</shipment_requesttype>
				<container>CP</container>
                <name>Stamps Example Shipping</name>
                <price>15.00</price>
                <title>Stamps Example</title>
                <type>I</type>
				<allowed_methods>
				US-FC,US-XM,US-MM,US-LM,US-PP,US-PS,US-CM,US-PM
				</allowed_methods>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
				<min_package_weight>0.1</min_package_weight>
                <max_package_weight>150</max_package_weight>
                <handling_type>F</handling_type>
                <handling_action>O</handling_action>
				<mode_xml>1</mode_xml>
                <is_account_live>0</is_account_live>
                <active_rma>0</active_rma>
                <is_online>1</is_online>
            </stamps>
        </carriers>
    </default>
</config>